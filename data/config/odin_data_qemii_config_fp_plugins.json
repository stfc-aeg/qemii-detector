        "frames": 1000,
        "dataset": 
        {
          "data" : 
          {
            "cmd": "create",
            "datatype": 1,
            "dims": [102, 288]                
          },
          "test" : 
          {
            "cmd": "create",
            "datatype": 1,
            "dims": [102, 288]                
          }
        }
      }
    },
    "processor_plugins":
    [
        {  
            "plugin": {
                "load": {
                    "index": "hdf",
